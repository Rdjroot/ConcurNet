{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build echoserver",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "${workspaceFolder}/build"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"],
            "detail": "Generated task by CMake Tools."
        },
        {
            "label": "Clean",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "${workspaceFolder}/build",
                "--target",
                "clean_all"
            ],
            "group": "build",
            "problemMatcher": []
        }
    ]
}
